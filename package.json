{
  "name": "micro-memfs",
  "version": "1.0.5",
  "description": "A simple, tiny, in-memory mock file system for all environments.",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "homepage": "https://github.com/awphi/micro-memfs",
  "scripts": {
    "build": "tsc",
    "test": "jest",
    "size": "size-limit",
    "prepublishOnly": "npm run test && npm run build && npm run size"
  },
  "keywords": [
    "fs",
    "file system",
    "memory fs",
    "browser fs"
  ],
  "size-limit": [
    {
      "path": "dist/index.js",
      "limit": "100B"
    }
  ],
  "author": "adamwph/awphi (https://adamw.ph)",
  "license": "MIT",
  "devDependencies": {
    "@size-limit/file": "^8.1.2",
    "@size-limit/webpack": "^8.1.2",
    "@types/jest": "^29.4.0",
    "jest": "^29.4.2",
    "ts-jest": "^29.0.5",
    "typescript": "^4.9.5"
  }
}
